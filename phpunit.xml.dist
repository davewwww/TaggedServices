<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="vendor/autoload.php" colors="true">
    <testsuites>
        <testsuite name="Tagged Services Test Suite">
            <directory>src/Dwo/TaggedServices</directory>
        </testsuite>
    </testsuites>

    <logging>
        <log type="coverage-clover" target="build/logs/clover.xml"/>
    </logging>

    <filter>
        <whitelist>
            <directory>src/Dwo</directory>
            <exclude>
                <directory>src/Dwo/*/Tests</directory>
            </exclude>
        </whitelist>
    </filter>

</phpunit>
